<?xml version='1.0' encoding='utf-8'?>
<widget id="com.numerolgy.insight" version="1.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Numerology Machine</name>
    <description>
        A numerology app.
    </description>
    <author email="steven.j.barker.jr@gmail.com" href="http://tekgno.org">
		Steven Barker Jr.
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
</widget>
